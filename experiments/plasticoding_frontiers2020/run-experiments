 #!/bin/bash




while true
	do
         for i in {1..20}; do
                ./revolve.py --experiment-name karines_experiments/data/plastic_big_$i --run $i --manager experiments/karines_experiments/plastic_big.py --n-cores 4 --port-start 11141  --evaluation-time 50;

                sleep 5s
	   done
done
